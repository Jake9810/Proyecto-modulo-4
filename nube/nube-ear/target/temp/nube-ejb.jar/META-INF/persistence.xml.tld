<?xml version="1.0" encoding="UTF-8"?>

<persistence>
    <persistence-unit name="nubeDS" transaction-type="JTA">
        <jta-data-source>java:/nubeDS</jta-data-source>
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <properties>
            <propertie name="hibernate.show_sql" value="true"/>
            <propertie name="hibernate.dialect" value="org.hibernate.dialect.MySQL8Dialect"/>
        </properties>
    </persistence-unit>

</persistence>
